+++ b/proxy/src/main/java/net/md_5/bungee/command/CommandPerms.java
@@ -13,7 +13,7 @@ public class CommandPerms extends Command
 
     public CommandPerms()
     {
-        super( "perms" );
+        super( "perms", "bungeecord.command.ip" ); // MikroCord - add missing permission node
     }
 
     @Override
